{
    "manifest_version": 2,

    "name": "plain twitter refresh",
    "description": "The simple extension that automatically updates Twitter feed.",
    "version": "1.0",
    "author": "Artyom Yagovdik <qweirdy@gmail.com>",
    "content_scripts": [
        {
            "matches": ["*://twitter.com/*"],
            "js": ["jquery-3.5.1.min.js","content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "click_good.png"
    },
    "icons": {
        "128": "click_good.png"
    }
}
